   "cell_type": "code",
   "execution_count": 25,
   "id": "5ae0add0",
   "metadata": {},
   "outputs": [],
   "source": [
    "def merge3columns(filename, docnameans, partiesans):\n",
    "    final_text= filename +' ' +docnameans+' '+partiesans\n",
    "    return final_text"
   ]
  },
